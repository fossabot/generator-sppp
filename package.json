{
  "name": "generator-sppp",
  "description": "SharePoint Pull-n-Push - Yeoman generator for client-side development",
  "version": "3.5.0",
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "main": "./app/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -p . && copyfiles ./src/templates/**/* ./app --up 1",
    "lint": "tslint -p .",
    "test": "mocha --opts test/mocha.opts || ECHO.",
    "test:manual": "ts-node ./test/manual/run.ts",
    "clean": "rimraf ./test/manual/tmp ./test/tmp ./app"
  },
  "bugs": {
    "url": "https://github.com/koltyakov/generator-sppp/issues"
  },
  "keywords": [
    "sharepoint",
    "assets",
    "yeoman-generator",
    "front-end",
    "scaffolders",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/generator-sppp.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/koltyakov/generator-sppp",
  "dependencies": {
    "colors": "^1.3.3",
    "dargs": "^6.1.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^4.0.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@pnp/pnpjs": "^1.3.2",
    "@types/chai": "^4.1.7",
    "@types/colors": "^1.2.1",
    "@types/dargs": "^6.1.3",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/rimraf": "^2.0.2",
    "@types/yeoman-generator": "^3.1.2",
    "@uifabric/icons": "^6.5.2",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.0",
    "mocha": "^6.1.4",
    "office-ui-fabric-react": "^6.184.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "sp-build-tasks": "^3.6.5",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.5"
  }
